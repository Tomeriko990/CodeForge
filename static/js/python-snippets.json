{
    "Print Statement": {
      "prefix": "print",
      "body": ["print($1)"],
      "description": "Print to console"
    },
    "If Statement": {
      "prefix": "if",
      "body": ["if $1:", "    $2"],
      "description": "Basic if statement"
    },
    "If-Else Statement": {
      "prefix": "ifelse",
      "body": ["if $1:", "    $2", "else:", "    $3"],
      "description": "If-else statement"
    },
    "For Loop (range)": {
      "prefix": "for",
      "body": ["for i in range($1):", "    $2"],
      "description": "For loop using range"
    },
    "While Loop": {
      "prefix": "while",
      "body": ["while $1:", "    $2"],
      "description": "While loop"
    },
    "Function Definition": {
      "prefix": "def",
      "body": ["def $1($2):", "    $3"],
      "description": "Function definition"
    },
    "Class Definition": {
      "prefix": "class",
      "body": ["class $1:", "    def __init__(self, $2):", "        $3"],
      "description": "Class definition with constructor"
    },
    "List Comprehension": {
      "prefix": "listcomp",
      "body": ["[$1 for $2 in $3]"],
      "description": "List comprehension"
    },
    "Main Guard": {
      "prefix": "main",
      "body": ["if __name__ == \"__main__\":", "    $1"],
      "description": "Main guard"
    },
    "Try-Except Block": {
      "prefix": "try",
      "body": ["try:", "    $1", "except $2 as e:", "    $3"],
      "description": "Try-except block"
    },
    "Import Statement": {
      "prefix": "imp",
      "body": ["import $1"],
      "description": "Import a module"
    },
    "From Import Statement": {
      "prefix": "from",
      "body": ["from $1 import $2"],
      "description": "From-import statement"
    },
    "Lambda Function": {
      "prefix": "lambda",
      "body": ["lambda $1: $2"],
      "description": "Lambda function"
    },
    "Dictionary Comprehension": {
      "prefix": "dictcomp",
      "body": ["{$1: $2 for $3 in $4}"],
      "description": "Dictionary comprehension"
    },
    "With Open File": {
      "prefix": "with",
      "body": ["with open('$1', '$2') as f:", "    $3"],
      "description": "With open file block"
    }
  }
  